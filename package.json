{
  "name": "vitepress-theme-lib",
  "version": "0.0.1",
  "description": "this is vitepress theme lib",
  "main": "index.js",
  "scripts": {
    "dev": "run-p docs:dev dev:theme watch:theme",
    "docs:dev": "vitepress dev docs --host --port 8090",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "dev:theme": "vue-tsc --noEmit -w -p src/theme",
    "build": "run-p build:theme copy:theme",
    "build:theme": "vue-tsc --noEmit -p src/theme",
    "copy:theme": "node scripts/copyClient.js",
    "watch:theme": "node scripts/watchAndCopy.js"
  },
  "keywords": [
    "vitepress",
    "docs",
    "dumi"
  ],
  "author": "aibayanyu",
  "license": "ISC",
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "chokidar": "^3.5.2",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.4",
    "markdown-it": "^12.2.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.4.4",
    "vitepress-custom": "^0.1.3",
    "vue-tsc": "^0.28.9"
  },
  "dependencies": {
    "vue": "^3.2.20"
  }
}
